.    c.container = 'vector'
.  elsif string.prefixed(c.name?'no-t-name',"list<")
.    c.container = 'list'
.  elsif string.prefixed(c.name?'no-t-name',"map<")
.    c.container = 'map'
.  else
.-    echo "No handling for Class of type $(c.name?'WhatNoName??') yet...."
.  endif
