.    copy f to s
.-   echo " Copying $(name(f):) ofType:$(f.type?'') -> $(f.name?'<undefined>':) into $(s.name:)..."
.  endfor
.  if s.name = 'in_addr'
.    s.json_type = 'InAddr'
.    s.default_value = '{ 0 }'
.  elsif name = 'char'
.    s.default_value = "{ '\0' }"
