.      st = root->GCC_XML->Struct(id = t.type)
.      t.json_type = 'Object'
.      t.default_value  = st.default_value?
.    else
.-      t.json_type_cast = tt.json_type_cast?
.-      t.json_type      = tt.json_type?
.-      t.default_value  = tt.default_value?
.    endif
.  else
